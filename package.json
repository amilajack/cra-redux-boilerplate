{
  "name": "cra-redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.3",
    "react-scripts": "3.1.2",
    "redux": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "bliss",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "react/prop-types": "off",
      "flowtype-errors/show-errors": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-bliss": "^4.6.0",
    "eslint-plugin-react": "^7.17.0"
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
